<?xml version='1.0' encoding='ISO-8859-1'?>

<pagetemplate name='Forest'>
	<uri>http://www.google.com/</uri>
	<version>1.1</version>
	<description>A new forest-green template</description>
	<author>Verde</author>
	<author-uri>http://www.blazingglory.org</author-uri>
	<tags>green, three-column</tags>
	<preview>green-screen.jpg</preview>

	<css src='color.css'/>
	<css src='style.css'/>
    <css src='global-for-template.css'/>
	<images src='images/'/>

	<content id="logo"		type="html"	src="logo.html" />
	<content id="links"     type="html"	src="links.html" />
	<content id="sidebar" 	type="html"	src="sidebar.html" />
	<content id="footer" 	type="html"	src="footer.html" />

	<pagebody>
		<div id="main">
			<content id='logo'>
				<img src='logo-*'/>
			</content>
			<content id='links'/>
			<div id='site_content'>
				<content id='sidebar' />
				<content id='content' />
			</div>
			<div id='clearfooter'>&#160;</div>
			<content id='footer'>(c) Someone.</content>
		</div>
	</pagebody>
</pagetemplate>
